#include "holberton.h"
/**
 *times_table - print times table
 *
 *
 */
void times_table(void)
{
	int i, j;
	int product;

	for (i = 0; i <= 9; i++)
	{
		for (j = 0; j <= 9; j++)
		{
			product = i * j;
			if (product <= 9)
			{
				if (j != 0)
				{
					_putchar(' ');
				}
				_putchar(mult + '0');
			}
			else
			{
				_putchar(product / 10 + '0');
				_putchar(product % 10 + '0');
			}
			if (j != 9)
			{
				_putchar(',');
				_putchar(' ');
			}
			else
			{
				_putchar('\n');
			}
		}
	}
}
