#include "holberton.h"
/**
 *puts2 - prints every other character
 *@str: string character
 *
 *Return (VOID)
 */
void puts2(char *str)
{
	int i, j;
	int count = 0;

	while (str[count] != '\0')
	{
		count++;
	}
        j = count - 1;
	for (i = 0; i < count; i = i + 2)
	{
		_putchar(str[i]);
	}
	_putchar('\n');
}
